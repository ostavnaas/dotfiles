
# https://learnxinyminutes.com/docs/yaml/
key: value
boolean: true
null_value: null
literal_block: |
  block of text
  with line breks preserved
folded_style: >
  block of text
  all single line


base: &base
  script:
    - echo "test"
extra: &extra
  key: value

my:
  <<: *base
  <<: *extra
  stage: deploy

description: >-
  This is a long description
  that spans multiple lines
  and is folded into a single line
